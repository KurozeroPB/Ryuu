    <PackageReference Include="Discord.Net.WebSocket" Version="2.2.0" />
    <PackageReference Include="IMDBCore" Version="1.0.2" />
    <PackageReference Include="Kitsu" Version="1.4.2" />
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp.Scripting" Version="3.7.0" />
    <PackageReference Include="Microsoft.CodeAnalysis.Scripting" Version="3.7.0" />
    <PackageReference Include="Microsoft.CodeAnalysis.Scripting.Common" Version="3.7.0" />
