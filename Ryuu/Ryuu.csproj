<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <ApplicationIcon>Ryuu.ico</ApplicationIcon>
    <ApplicationVersion>1.0.0</ApplicationVersion>
    <Authors>Pepijn</Authors>
